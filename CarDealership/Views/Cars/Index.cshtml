@using CarDealership.Models;
<h1>CarDealership</h1>
@if (Model.Count == 0)
{
  <p>There are no cars in the list.</p>
}
<ul>
  @foreach (Car car in Model)
  {
    <h1>@car.MakeModel</h1>
    <p>price $@car.Price</p>
    <p>Miles @car.Miles</p>
    <br>
  }
</ul>

<a href="/cars/new">Add a new car.</a>